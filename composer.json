{
    "name": "krisstwo/odtphp-bundle",
    "description": "A bundle to integrate odtphp in Symfony framework",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed KRISTOU",
            "email": "krisstwo@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/james2001/odtphp"
        }
    ],
    "require": {
        "cybermonde/odtphp": "dev-master@dev",
        "symfony/http-kernel": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/config": "^4.4"
    },
    "require-dev": {
        "cybermonde/odtphp": "dev-master@dev",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": { "Krisstwo\\OdtPHPBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Krisstwo\\OdtPHPBundle\\Tests\\": "tests/" }
    }
}
